{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "nodeJs typescript API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:local": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/index",
