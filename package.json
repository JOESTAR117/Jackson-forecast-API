  "bugs": {
    "url": "https://github.com/JOESTAR117/jackson-forecast-API/issues"
  },
  "homepage": "https://github.com/JOESTAR117/jackson-forecast-API#readme",
  "devDependencies": {
    "@eslint/eslintrc": "^1.4.1",
    "@types/express": "^4.17.16",
    "@types/jest": "^29.2.6",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "jest": "^29.4.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "tsconfig.json": "^1.0.11",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
